{
	"name": "metafoot",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@jackfranklin/test-data-bot": "^1.3.0",
		"@material-ui/core": "^4.12.3",
		"@material-ui/icons": "^4.11.2",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.11.7",
		"@types/react": "^17.0.34",
		"@types/react-dom": "^17.0.11",
		"lodash": "^4.17.21",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "^4.0.3",
		"styled-components": "^5.3.3",
		"typescript": "^4.4.4",
		"web-vitals": "^2.1.2",
		"workbox-background-sync": "^6.3.0",
		"workbox-broadcast-update": "^6.3.0",
		"workbox-cacheable-response": "^6.3.0",
		"workbox-core": "^6.3.0",
		"workbox-expiration": "^6.3.0",
		"workbox-google-analytics": "^6.3.0",
		"workbox-navigation-preload": "^6.3.0",
		"workbox-precaching": "^6.3.0",
		"workbox-range-requests": "^6.3.0",
		"workbox-routing": "^6.3.0",
		"workbox-strategies": "^6.3.0",
		"workbox-streams": "^6.3.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "npm run test:prebuild && react-scripts build",
		"test": "react-scripts test --runInBand",
		"test:coverage": "react-scripts test --coverage --runInBand --watchAll=false",
		"test:prebuild": "react-scripts test --watchAll=false",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app",
		"rules": {
			"react/prop-types": "warn"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.15.0",
		"@testing-library/react": "^12.1.2",
		"@testing-library/react-hooks": "^7.0.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/lodash": "^4.14.176",
		"@types/styled-components": "^5.1.15",
		"babel-plugin-styled-components": "^1.13.3",
		"husky": "^7.0.4",
		"jest-styled-components": "^7.0.5",
		"prettier": "2.4.1",
		"pretty-quick": "^3.1.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"jest": {
		"collectCoverageFrom": [
			"**/src/**/*.js"
		],
		"moduleNameMapper": {
			"\\..css$": "identity-obj-proxy"
		},
		"displayName": "client",
		"coverageThreshold": {
			"global": {
				"statements": 0,
				"branches": 0,
				"functions": 0,
				"lines": 0
			}
		}
	}
}
